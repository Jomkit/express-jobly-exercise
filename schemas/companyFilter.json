{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "name",
        "minEmployees",
        "maxEmployees"
    ],
    "properties": {
        "name": {
            "type": "string",
            "default": "",
            "title": "The name Schema",
            "examples": [
                "John"
            ]
        },
        "minEmployees": {
            "type": "integer",
            "default": 0,
            "title": "The minEmployees Schema",
            "examples": [
                30
            ]
        },
        "maxEmployees": {
            "type": "integer",
            "default": 0,
            "title": "The maxEmployees Schema",
            "examples": [
                300
            ]
        }
    },
    "examples": [{
        "name": "John",
        "minEmployees": 30,
        "maxEmployees": 300
    }]
}